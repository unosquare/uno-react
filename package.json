{
  "name": "uno-react",
  "version": "0.10.7",
  "description": "Common functions, HOCs and hooks for React.",
  "main": "lib/index.js",
  "scripts": {
    "prebuild:docs": "npx shx rm -rf docs/*",
    "build:docs": "npx parcel build ./srcdocs/index.html --no-source-maps --public-url /uno-react/ -d ./docs",
    "start": "npx parcel ./srcdocs/index.html --no-source-maps --port 8080 --open",
    "test": "node ./test",
    "build": "tsc",
    "lint": "tslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "tslint src/**/*.ts src/**/*.tsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unosquare/uno-react.git"
  },
  "keywords": [
